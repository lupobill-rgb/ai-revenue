{
  "id": "ai_cro",
  "name": "AI CRO Module",
  "version": "2.0.0",
  "description": "AI-powered Chief Revenue Officer module for targets, forecasting, deal reviews, and revenue optimization.",
  "entry_agent": "CRO Orchestrator",
  "orchestrator_prompt": "/agents/cro/orchestrator.system.prompt.txt",
  "modes": ["setup", "forecast", "deal_review", "optimization"],
  "requires": ["supabase", "kernel", "ai_gateway"],
  "docs": [
    "/knowledge/cro/backend_requirements.md",
    "/knowledge/cro/frontend_requirements.md"
  ],
  "schemas": [
    "cro_targets",
    "cro_forecasts",
    "cro_deal_reviews",
    "cro_recommendations"
  ],
  "ui": {
    "basePath": "/cro",
    "primaryScreens": [
      "Dashboard",
      "Targets",
      "Forecasts",
      "Deal Reviews",
      "Recommendations"
    ]
  },
  "permissions": {
    "tenant": true,
    "role": ["admin", "sales"]
  },
  "agents": {
    "pipelineIntake": {
      "prompt": "/agents/cro/setup/pipelineIntake.prompt.txt",
      "function": "cro-pipeline-intake"
    },
    "forecastPlanner": {
      "prompt": "/agents/cro/forecast/forecastPlanner.prompt.txt",
      "function": "cro-forecast-planner"
    },
    "dealReviewer": {
      "prompt": "/agents/cro/deals/dealReview.prompt.txt",
      "function": "cro-deal-review"
    },
    "revenueMoves": {
      "prompt": "/agents/cro/optimization/revenueMoves.prompt.txt",
      "function": "cro-revenue-moves"
    }
  },
  "_status": "active"
}
